+package BusinessLogik;
+
+public class Getraenk {
+    private String name;
+    private double preis;
+    public Getraenk(String name, double preis) {
+        this.name = name;
+        this.preis = preis;
+    }
+    public String getName() {
+        return name;
+    }
+    public void setName(String neuerNameGetraenk){
+        name=neuerNameGetraenk;
+    }
+    public double getPreis() {
+        return preis;
+    }
+    public void setPreis(double neuerPreisgetraenk){
+        preis=neuerPreisgetraenk;
+    }
+}
